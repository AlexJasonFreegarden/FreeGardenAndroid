<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.henallux.freegarden">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.NewAccountActivity" />
        <activity android:name=".View.NewUserGoalActivity" />
        <activity android:name=".View.ProfileActivity" />
        <activity android:name=".View.GardenRegisterActivity" />
        <activity android:name=".View.GardenProfile" />
        <activity android:name=".View.TestActivity"></activity>
    </application>

</manifest>